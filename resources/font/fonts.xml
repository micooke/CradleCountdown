<!-- 
# Note : In DigitaltSmallErosion.fnt; # = message icon, $ = heart icon from LigatureSymbols font
 
Heartrate and Message icons from open SIL font : http://kudakurage.com/ligature_symbols/
Digitalt open SIL font from : http://www.glukfonts.pl/font.php?font=Digitalt
-->
<fonts>
	<font id="DigitaltSmall" filename="DigitaltSmallErosion.fnt" antialias="true" filter="ABCDEFGHIJKLMNOPQRSTUVWXYZ 0123456789%#$:"/>
	<font id="DigitaltMedium" filename="DigitaltMedium.fnt" antialias="true"/>
	<font id="Digitalt" filename="Digitalt.fnt" antialias="true" filter=" 0123456789"/>
	<font id="DigitaltChecker2" filename="DigitaltChecker2.fnt" antialias="true" filter="0"/>
</fonts>